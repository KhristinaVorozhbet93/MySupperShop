@page "/catalog"
@using MySupperShop.Components
@using MySupperShop.Interfaces;
@using MySupperShop.Models;
@inject ICatalog catalog

<MudDrawerHeader Class="text">Каталог магазина продуктов</MudDrawerHeader>
<MudGrid Style="background-color:#97fdff">
    @foreach (var product in _products)
    {
        <ProductItem Product="product" />
    }
</MudGrid>

